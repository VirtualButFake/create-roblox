{
	"name": "create-rblx",
	"version": "0.1.1",
	"description": "A dynamic scaffolding utility for Roblox projects.",
	"main": "dist/index.cjs",
	"type": "module",
	"scripts": {
		"build": "npx esbuild src/index.ts --bundle --outdir=dist --platform=node --minify --out-extension:.js=.cjs",
		"start": "npm run build && node dist/index.cjs",
		"dev": "npx esbuild src/index.ts --bundle --outdir=dist --platform=node --watch"
	},
	"bin": {
		"create-roblox": "./bin.cjs"
	},
	"author": "VirtualButFake",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/VirtualButFake/create-rblx.git"
	},
	"bugs": {
		"url": "https://github.com/VirtualButFake/create-rblx/issues"
	},
	"homepage": "https://github.com/VirtualButFake/create-rblx#readme",
	"dependencies": {
		"@types/fs-extra": "^11.0.4",
		"@types/inquirer": "^9.0.7",
		"chalk": "^5.3.0",
		"fs-extra": "^11.2.0",
		"inquirer": "^9.2.14",
		"winston": "^3.11.0"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^7.1.0",
		"eslint": "^8.57.0"
	}
}
